{{outlet}}
{{#admin-user-form model=model
  saveUser='saveUser'
  store=store}}
  <label class="label title is-5">Password</label>
  <p class="control">
    {{input class="input" type="password" name='title' value=model.password placeholder="Enter password"}}
  </p>
  <label class="label title is-5">Confirm Password</label>
  <p class="control">
    {{input class="input" type="password" name='title' value=model.passwordConfirmation placeholder="Enter password"}}
  </p>
{{/admin-user-form}}