{{outlet}}
<section class='section'>
    <div class="container content">
      {{#with model as |page|}}
        {{{page.renderedContent}}}
        {{#if page.enableComments}}
          <hr/>
          {{#comment-form store=store page=page}}{{/comment-form}}
          {{#comment-container comments=page.comments}}{{/comment-container}}
        {{/if}}
      {{/with}}
    </div>
</section>